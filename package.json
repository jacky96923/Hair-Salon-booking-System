{
  "name": "bad_project",
  "version": "1.0.0",
  "description": "bad_project Let's go boys",
  "main": "index.js",
  "compilerOptions": {
    "strict": true,
    "module": "es2022",
    "target": "es2022",
    "lib": [
      "es2022"
    ],
    "esModuleInterop": true,
    "moduleResolution": "node",
    "noImplicitReturns": true,
    "noUnusedLocals": true,
    "outDir": "dist"
  },
  "exclude": [
    "dist"
  ],
  "scripts": {
    "test": "npm run type-check && npm run jest",
    "type-check": "tsc --noEmit",
    "jest": "jest --runInBand",
    "build": "tsc -p .",
    "start": "ts-node-dev server.ts",
    "db:dev": "run-s db:update db:plan db:update",
    "db:plan": "auto-migrate pg < erd.txt",
    "db:update": "knex migrate:latest && erd-to-types < erd.txt > types.ts"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@types/express-session": "^1.17.10",
    "@types/node": "^20.10.6",
    "bcryptjs": "^2.4.3",
    "dayjs": "^1.11.10",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-session": "^1.17.3",
    "knex": "^3.1.0",
    "pg": "^8.11.3",
    "populate-env": "^2.0.0",
    "replicate": "^0.25.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/express": "^4.17.21",
    "@types/express-session": "^1.17.10",
    "@types/knex": "^0.16.1",
    "@types/node": "^20.10.6",
    "@types/pg": "^8.10.9",
    "jest": "^29.7.0",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.3.3"
  }
}
